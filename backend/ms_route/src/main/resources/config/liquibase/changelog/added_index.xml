<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
  <!-- 0) Turn off FK checks for bulk import -->
  <changeSet id="idx-route-route_code" author="jhipster">
    <preConditions onFail="MARK_RAN">
      <not>
        <indexExists indexName="idx_route_route_code" tableName="route"/>
      </not>
    </preConditions>
    <createIndex indexName="idx_route_route_code" tableName="route">
      <column name="route_code"/>
    </createIndex>
  </changeSet>
  <changeSet id="idx-district-district_code" author="jhipster">
    <preConditions onFail="MARK_RAN">
      <not>
        <indexExists indexName="idx_district_code" tableName="district"/>
      </not>
    </preConditions>
    <createIndex indexName="idx_district_code" tableName="district">
      <column name="district_code"/>
    </createIndex>
  </changeSet>
  <changeSet id="idx-province-province_code" author="jhipster">
    <preConditions onFail="MARK_RAN">
      <not>
        <indexExists indexName="idx_province_code" tableName="province"/>
      </not>
    </preConditions>
    <createIndex indexName="idx_province_code" tableName="province">
      <column name="province_code"/>
    </createIndex>
  </changeSet>
</databaseChangeLog>